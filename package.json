{
  "name": "node-test",
  "version": "1.0.0",
  "description": "Testing with node, a practice project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "set NODE_ENV=development&&nodemon index",
    "test": "jest --verbose",
    "test:watch": "jest -i --watchAll",
    "test:coverage": "jest -i --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kathemica/node-test.git"
  },
  "keywords": [
    "node",
    "test",
    "jest"
  ],
  "author": "Katherine Aguirre",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kathemica/node-test/issues"
  },
  "homepage": "https://github.com/kathemica/node-test#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.4",
    "@types/jest": "^26.0.23",
    "babel-jest": "^27.0.1",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
